      function searchGoogle() {
        var keyword = $($('.search-header-input')[0]).val();
        if (keyword === "") {
          keyword = $($('.search-header-input')[1]).val();
        };
        window.location.href="http://www.google.com/search?q=site:cacm.acm.org+" + keyword;
        return false;
      }
    </script>
